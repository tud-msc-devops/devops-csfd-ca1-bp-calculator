{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "bp_webapp_plan_name": {
      "type": "String"
    },
    "bp_webapp_plan_tier": {
      "type": "String"
    },
    "bp_webapp_plan_size": {
      "type": "String"
    },
    "bp_webapp_plan_family": {
      "type": "String"
    },
    "bp_webapp_plan_capacity": {
      "type": "String"
    }
  },
  "variables": {

  },
  "resources": [
    {
      "type": "Microsoft.Web/serverfarms",
      "apiVersion": "2024-11-01",
      "name": "[parameters('bp_webapp_plan_name')]",
      "location": "Norway East",
      "sku": {
        "name": "F1",
        "tier": "Free",
        "size": "F1",
        "family": "F",
        "capacity": 0
      },
      "kind": "app",
      "properties": {
        "perSiteScaling": false,
        "elasticScaleEnabled": false,
        "maximumElasticWorkerCount": 1,
        "isSpot": false,
        "reserved": false,
        "isXenon": false,
        "hyperV": false,
        "targetWorkerCount": 0,
        "targetWorkerSizeId": 0,
        "zoneRedundant": false,
        "asyncScalingEnabled": false
      }
    }
  ],
  "outputs": {

  }
}
